{
  "name": "rdygo-service",
  "version": "1.0.0",
  "description": "Services for all rdygo clients",
  "main": "bin/rdygo-service.js",
  "scripts": {
    "lint": "eslint lib test server",
    "test": "npm run lint && CONFIGURATION=config.json ENVIRONMENT=prod istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test-local": "npm run lint && CONFIGURATION=config.json ENVIRONMENT=local istanbul cover _mocha -- -t 10000 -R spec test",
    "start": "./bin/rdygo-service.js -c config.json -e prod"
  },
  "engines": {
    "node": ">= 6.2.2",
    "npm": ">= 3.9.5"
  },
  "keywords": [
    "rdygo",
    "api",
    "node",
    "js"
  ],
  "author": "Luis Valdovinos",
  "license": "ISC",
  "devDependencies": {
    "coveralls": "^2.11.12",
    "eslint": "^2.11.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "^1.2.0",
    "should": "^7.1.0",
    "supertest": "^1.1.0"
  },
  "dependencies": {
    "bcrypt": "^0.8.7",
    "body-parser": "1.14.1",
    "commander": "2.9.0",
    "iron": "^4.0.1",
    "moment": "^2.13.0",
    "mongoose": "^4.5.8",
    "my-config": "2.0.0",
    "restify": "^4.1.1",
    "winston": "^2.2.0"
  }
}
